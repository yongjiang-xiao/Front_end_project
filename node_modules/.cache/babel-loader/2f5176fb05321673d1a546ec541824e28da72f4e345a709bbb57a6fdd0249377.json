{"ast":null,"code":"export default {\n  name: 'ShuttleBus',\n  data() {\n    return {\n      // 校区选择\n      fromCampus: 'balitai',\n      toCampus: 'jinnan',\n      // 日期类型\n      scheduleType: 'weekday',\n      // weekday, weekend, holiday\n\n      // 当前日期\n      currentDate: new Date(),\n      // 校车时刻表数据\n      busSchedules: {\n        balitaiToJinnan: {\n          weekday: {\n            express: {\n              route: '南开大学→天津图书馆→水上公园西路地铁站→天塔地铁站→南开大学新校西门→天南大新址公交站',\n              price: '5元',\n              times: ['7:50', '8:45', '9:00', '9:30', '10:00', '10:50', '11:30', '12:50', '13:10', '13:30', '13:50', '14:10', '14:50', '15:20', '15:50', '16:10', '16:50', '17:10', '17:30', '17:50', '18:20', '19:00', '19:20', '19:45', '20:00', '20:15', '20:30']\n            },\n            direct: {\n              route: '南开大学→一食堂（津南校区）→梨树园（津南校区）',\n              price: '5元',\n              times: ['6:30', '6:45', '7:00', '7:15', '7:30', '7:45', '8:00', '8:10', '8:20', '8:30', '10:30', '12:20', '12:30', '12:40', '14:30', '16:30', '18:30', '21:20']\n            }\n          },\n          weekend: {\n            express: {\n              route: '南开大学→天津图书馆→水上公园西路地铁站→天塔地铁站→南开大学新校西门→天南大新址公交',\n              price: '5元',\n              firstBus: '6:30',\n              lastBus: '21:15',\n              interval: '20-30分钟'\n            }\n          },\n          holiday: {\n            direct: {\n              route: '南开大学→一食堂（津南校区）→梨树园（津南校区）',\n              price: '5元',\n              times: ['6:00', '12:30', '18:30', '21:15']\n            }\n          }\n        },\n        jinnanToBalitai: {\n          weekday: {\n            express: {\n              route: '天南大新址公交站→南开大学新校西门→天塔地铁站→水上公园西路地铁站→天津图书馆→南开大学',\n              price: '5元',\n              times: ['6:30', '6:40', '6:50', '7:10', '7:40', '8:00', '8:30', '9:00', '9:40', '10:10', '10:30', '11:00', '11:20', '11:40', '12:10', '13:10', '14:00', '14:50', '15:10', '15:30', '15:50', '17:50', '18:30', '18:50', '19:10', '19:50', '20:10']\n            },\n            direct: {\n              route: '梨树园（津南校区）→一食堂（津南校区）→天塔地铁站→水上公园西路地铁站→天津图书馆→南开大学',\n              price: '5元',\n              times: ['6:35', '9:20', '12:20', '12:30', '12:40', '14:30', '16:00', '16:10', '16:25', '16:40', '17:00', '17:10', '17:20', '17:30', '18:10', '19:30', '20:35', '21:15']\n            }\n          },\n          weekend: {\n            express: {\n              route: '天南大新址公交站→南开大学新校西门→天塔地铁站→水上公园西路地铁站→天津图书馆→南开大学',\n              price: '5元',\n              firstBus: '6:30',\n              lastBus: '21:15',\n              interval: '20-30分钟'\n            }\n          },\n          holiday: {\n            direct: {\n              route: '梨树园（津南校区）→一食堂（津南校区）→天塔地铁站→水上公园西路地铁站→天津图书馆→南开大学',\n              price: '5元',\n              times: ['8:00', '12:30', '18:00', '21:15']\n            }\n          }\n        },\n        balitaiToTeda: {\n          weekday: {\n            route: '西门二道岗西区公寓→游泳馆东侧停车场→泰达学院钟楼',\n            price: '免费',\n            times: [{\n              departure: '6:25',\n              location: '西门二道岗西区公寓前'\n            }, {\n              departure: '6:30',\n              location: '游泳馆东侧停车场'\n            }, {\n              departure: '7:50',\n              location: '西门二道岗西区公寓前'\n            }, {\n              departure: '8:00',\n              location: '游泳馆东侧停车场'\n            }, {\n              departure: '10:00',\n              location: '西门二道岗西区公寓前'\n            }, {\n              departure: '10:10',\n              location: '游泳馆东侧停车场'\n            }, {\n              departure: '12:30',\n              location: '西门二道岗西区公寓前'\n            }, {\n              departure: '12:40',\n              location: '游泳馆东侧停车场'\n            }, {\n              departure: '17:20',\n              location: '西门二道岗西区公寓前'\n            }, {\n              departure: '17:30',\n              location: '游泳馆东侧停车场'\n            }]\n          },\n          weekend: {\n            route: '西门二道岗西区公寓→游泳馆东侧停车场→泰达学院钟楼',\n            price: '免费',\n            times: [{\n              departure: '17:20',\n              location: '西门二道岗西区公寓'\n            }, {\n              departure: '17:30',\n              location: '游泳馆东侧停车场'\n            }]\n          }\n        },\n        tedaToBalitai: {\n          weekday: {\n            route: '泰达学院钟楼→八里台校区',\n            price: '免费',\n            times: ['6:30']\n          },\n          weekend: {\n            route: '泰达学院钟楼→八里台校区',\n            price: '免费',\n            times: ['6:30']\n          }\n        },\n        jinnanToTeda: {\n          weekday: {\n            route: '津南校区梨树园→泰达学院钟楼',\n            price: '免费',\n            times: ['6:30', '8:30', '12:30', '18:00']\n          },\n          weekend: {\n            route: '津南校区梨树园→泰达学院钟楼',\n            price: '免费',\n            times: ['8:00', '11:00', '15:00', '18:00']\n          }\n        },\n        tedaToJinnan: {\n          weekday: {\n            route: '泰达学院钟楼→津南校区梨树园',\n            price: '免费',\n            times: ['6:30', '8:30', '12:30', '17:00']\n          },\n          weekend: {\n            route: '泰达学院钟楼→津南校区梨树园',\n            price: '免费',\n            times: ['8:00', '11:00', '15:00', '17:00']\n          }\n        }\n      },\n      // 校区名称映射\n      campusNames: {\n        balitai: '八里台校区',\n        jinnan: '津南校区',\n        teda: '泰达校区'\n      }\n    };\n  },\n  computed: {\n    // 当前路线\n    currentRoute() {\n      return `${this.fromCampus}To${this.toCampus.charAt(0).toUpperCase() + this.toCampus.slice(1)}`;\n    },\n    // 路线标题\n    routeTitle() {\n      return `${this.campusNames[this.fromCampus]} → ${this.campusNames[this.toCampus]}`;\n    },\n    // 当前日期类型文本\n    scheduleTypeText() {\n      switch (this.scheduleType) {\n        case 'weekday':\n          return '工作日';\n        case 'weekend':\n          return '非工作日';\n        case 'holiday':\n          return '节假日';\n        default:\n          return '工作日';\n      }\n    },\n    // 当前日期文本\n    currentDateText() {\n      const options = {\n        year: 'numeric',\n        month: 'long',\n        day: 'numeric',\n        weekday: 'long'\n      };\n      return this.currentDate.toLocaleDateString('zh-CN', options);\n    },\n    // 当前时刻表数据\n    currentScheduleData() {\n      const routeData = this.busSchedules[this.currentRoute];\n      if (!routeData) return null;\n      return routeData[this.scheduleType];\n    },\n    // 是否需要显示上车地点列\n    needLocationColumn() {\n      if (!this.currentScheduleData || !this.currentScheduleData.times) return false;\n      return Array.isArray(this.currentScheduleData.times) && this.currentScheduleData.times.length > 0 && typeof this.currentScheduleData.times[0] === 'object' && this.currentScheduleData.times[0].departure;\n    }\n  },\n  mounted() {\n    // 根据星期几设置默认的日期类型\n    const dayOfWeek = this.currentDate.getDay();\n    if (dayOfWeek === 0 || dayOfWeek === 6) {\n      this.scheduleType = 'weekend';\n    }\n  },\n  methods: {\n    // 交换校区\n    exchangeCampus() {\n      const temp = this.fromCampus;\n      this.fromCampus = this.toCampus;\n      this.toCampus = temp;\n    },\n    // 查询班次\n    querySchedule() {\n      if (this.fromCampus === this.toCampus) {\n        this.$message({\n          message: '请选择不同的出发和目的校区',\n          type: 'error'\n        });\n        return;\n      }\n      this.$message({\n        message: '已更新路线时刻表',\n        type: 'success'\n      });\n    },\n    // 改变日期类型\n    changeScheduleType(type) {\n      this.scheduleType = type;\n    },\n    // 获取状态样式类\n    getStatusClass(index, total) {\n      if (index === total - 1) {\n        return 'status-last';\n      }\n      return 'status-available';\n    },\n    // 获取状态文本\n    getStatusText(index, total) {\n      if (index === total - 1) {\n        return '末班车';\n      }\n      return '正常';\n    }\n  }\n};","map":{"version":3,"names":["name","data","fromCampus","toCampus","scheduleType","currentDate","Date","busSchedules","balitaiToJinnan","weekday","express","route","price","times","direct","weekend","firstBus","lastBus","interval","holiday","jinnanToBalitai","balitaiToTeda","departure","location","tedaToBalitai","jinnanToTeda","tedaToJinnan","campusNames","balitai","jinnan","teda","computed","currentRoute","charAt","toUpperCase","slice","routeTitle","scheduleTypeText","currentDateText","options","year","month","day","toLocaleDateString","currentScheduleData","routeData","needLocationColumn","Array","isArray","length","mounted","dayOfWeek","getDay","methods","exchangeCampus","temp","querySchedule","$message","message","type","changeScheduleType","getStatusClass","index","total","getStatusText"],"sources":["C:\\Users\\刘新康\\Desktop\\大二上课程\\前端开发实训\\大作业\\Front_end_project\\vue_version\\src\\views\\life\\ShuttleBus.vue"],"sourcesContent":["<template>\n  <div class=\"shuttle-bus\">\n    <div class=\"page-header\">\n      <div class=\"breadcrumb\">\n        <router-link to=\"/life\">生活服务</router-link>\n        <span class=\"separator\">/</span>\n        <span class=\"current\">校车查询</span>\n      </div>\n      <h1 class=\"page-title\">校车查询</h1>\n      <p class=\"page-description\">查询南开大学三个校区之间的校车班次和时间安排</p>\n    </div>\n\n    <div class=\"shuttle-bus-container\">\n      <!-- 校区选择和路线查询 -->\n      <div class=\"route-query-section\">\n        <div class=\"campus-selection\">\n          <h2>选择校区路线</h2>\n          <div class=\"campus-selector\">\n            <div class=\"campus-from\">\n              <label for=\"fromCampus\">出发校区</label>\n              <select id=\"fromCampus\" class=\"campus-select\" v-model=\"fromCampus\">\n                <option value=\"balitai\">八里台校区</option>\n                <option value=\"jinnan\">津南校区</option>\n                <option value=\"teda\">泰达校区</option>\n              </select>\n            </div>\n            <div class=\"exchange-icon\" @click=\"exchangeCampus\">\n              <i class=\"fas fa-arrow-right-arrow-left\"></i>\n            </div>\n            <div class=\"campus-to\">\n              <label for=\"toCampus\">目的校区</label>\n              <select id=\"toCampus\" class=\"campus-select\" v-model=\"toCampus\">\n                <option value=\"jinnan\">津南校区</option>\n                <option value=\"balitai\">八里台校区</option>\n                <option value=\"teda\">泰达校区</option>\n              </select>\n            </div>\n          </div>\n          <button class=\"btn btn-primary\" @click=\"querySchedule\">\n            <i class=\"fas fa-search\"></i> 查询班次\n          </button>\n        </div>\n\n        <!-- 校车信息提示 -->\n        <div class=\"bus-info-tips\">\n          <div class=\"tips-header\">\n            <i class=\"fas fa-info-circle\"></i>\n            <h3>校车服务提示</h3>\n          </div>\n          <div class=\"tips-content\">\n            <ul>\n              <li>校车服务时间：6:00-21:30（不同路线时间不同）</li>\n              <li>八里台校区与津南校区之间票价5元，其他路线免费</li>\n              <li>八里台校区至泰达校区有两个上车地点：西门二道岗西区公寓前和游泳馆东侧停车场</li>\n              <li>工作日、周末和节假日班次时间不同，请选择正确的日期类型</li>\n              <li>非工作日八里台-津南快线发车间隔为20-30分钟</li>\n              <li>校车可能会因天气或特殊情况调整，请留意实时通知</li>\n            </ul>\n          </div>\n        </div>\n      </div>\n\n      <!-- 日期类型选择 -->\n      <div class=\"schedule-type-section\">\n        <div class=\"schedule-tabs\">\n          <div \n            class=\"tab-btn\" \n            :class=\"{ active: scheduleType === 'weekday' }\" \n            @click=\"changeScheduleType('weekday')\"\n          >\n            <i class=\"fas fa-calendar-day\"></i>\n            <span>工作日排班</span>\n          </div>\n          <div \n            class=\"tab-btn\" \n            :class=\"{ active: scheduleType === 'weekend' }\" \n            @click=\"changeScheduleType('weekend')\"\n          >\n            <i class=\"fas fa-calendar-week\"></i>\n            <span>非工作日排班</span>\n          </div>\n          <div \n            class=\"tab-btn\" \n            :class=\"{ active: scheduleType === 'holiday' }\" \n            @click=\"changeScheduleType('holiday')\"\n          >\n            <i class=\"fas fa-calendar-star\"></i>\n            <span>节假日排班</span>\n          </div>\n        </div>\n      </div>\n\n      <!-- 校车时刻表 -->\n      <div class=\"schedule-section\">\n        <div class=\"schedule-header\">\n          <h2>{{ routeTitle }}</h2>\n          <div class=\"schedule-info\">\n            <span class=\"schedule-type\">{{ scheduleTypeText }}</span>\n            <span class=\"schedule-date\">{{ currentDateText }}</span>\n          </div>\n        </div>\n\n        <div class=\"schedule-tabs-content\">\n          <!-- 时刻表内容 -->\n          <div class=\"tab-content active\">\n            <div v-if=\"currentScheduleData\">\n              <!-- 复杂时刻表（包含快线和点对点） -->\n              <div v-if=\"currentScheduleData.express || currentScheduleData.direct\">\n                <!-- 快线时刻表 -->\n                <div v-if=\"currentScheduleData.express\" class=\"schedule-subsection\">\n                  <h4 class=\"route-type-title\">快线</h4>\n                  <div class=\"route-info\">\n                    <div class=\"route-path\">{{ currentScheduleData.express.route }}</div>\n                    <div class=\"route-price\">票价：{{ currentScheduleData.express.price }}</div>\n                  </div>\n                  <div class=\"time-table\">\n                    <div class=\"table-header\">\n                      <div class=\"header-cell\">发车时间</div>\n                      <div class=\"header-cell\">备注</div>\n                    </div>\n                    <div class=\"table-body\">\n                      <div v-if=\"currentScheduleData.express.times && Array.isArray(currentScheduleData.express.times)\">\n                        <div \n                          v-for=\"(time, index) in currentScheduleData.express.times\" \n                          :key=\"index\" \n                          class=\"table-row\"\n                        >\n                          <div class=\"cell time-cell\">{{ time }}</div>\n                          <div class=\"cell status-cell\">\n                            <span class=\"status\" :class=\"getStatusClass(index, currentScheduleData.express.times.length)\">\n                              {{ getStatusText(index, currentScheduleData.express.times.length) }}\n                            </span>\n                          </div>\n                        </div>\n                      </div>\n                      <div v-else-if=\"currentScheduleData.express.firstBus && currentScheduleData.express.lastBus\">\n                        <div class=\"table-row\">\n                          <div class=\"cell time-cell\">{{ currentScheduleData.express.firstBus }} - {{ currentScheduleData.express.lastBus }}</div>\n                          <div class=\"cell status-cell\">\n                            <span class=\"status status-available\">发车间隔：{{ currentScheduleData.express.interval }}</span>\n                          </div>\n                        </div>\n                      </div>\n                    </div>\n                  </div>\n                </div>\n                \n                <!-- 点对点时刻表 -->\n                <div v-if=\"currentScheduleData.direct\" class=\"schedule-subsection\">\n                  <h4 class=\"route-type-title\">点对点</h4>\n                  <div class=\"route-info\">\n                    <div class=\"route-path\">{{ currentScheduleData.direct.route }}</div>\n                    <div class=\"route-price\">票价：{{ currentScheduleData.direct.price }}</div>\n                  </div>\n                  <div class=\"time-table\">\n                    <div class=\"table-header\">\n                      <div class=\"header-cell\">发车时间</div>\n                      <div class=\"header-cell\">备注</div>\n                    </div>\n                    <div class=\"table-body\">\n                      <div \n                        v-for=\"(time, index) in currentScheduleData.direct.times\" \n                        :key=\"index\" \n                        class=\"table-row\"\n                      >\n                        <div class=\"cell time-cell\">{{ time }}</div>\n                        <div class=\"cell status-cell\">\n                          <span class=\"status\" :class=\"getStatusClass(index, currentScheduleData.direct.times.length)\">\n                            {{ getStatusText(index, currentScheduleData.direct.times.length) }}\n                          </span>\n                        </div>\n                      </div>\n                    </div>\n                  </div>\n                </div>\n              </div>\n              \n              <!-- 单一路线时刻表 -->\n              <div v-else class=\"schedule-subsection\">\n                <div class=\"route-info\">\n                  <div class=\"route-path\">{{ currentScheduleData.route }}</div>\n                  <div class=\"route-price\">票价：{{ currentScheduleData.price }}</div>\n                </div>\n                <div class=\"time-table\">\n                  <div class=\"table-header\">\n                    <div v-if=\"needLocationColumn\" class=\"header-cell\">发车时间</div>\n                    <div v-if=\"needLocationColumn\" class=\"header-cell\">上车地点</div>\n                    <div v-if=\"!needLocationColumn\" class=\"header-cell\">发车时间</div>\n                    <div class=\"header-cell\">备注</div>\n                  </div>\n                  <div class=\"table-body\">\n                    <div v-if=\"needLocationColumn\">\n                      <div \n                        v-for=\"(timeInfo, index) in currentScheduleData.times\" \n                        :key=\"index\" \n                        class=\"table-row\"\n                      >\n                        <div class=\"cell time-cell\">{{ timeInfo.departure }}</div>\n                        <div class=\"cell\">{{ timeInfo.location }}</div>\n                        <div class=\"cell status-cell\">\n                          <span class=\"status\" :class=\"getStatusClass(index, currentScheduleData.times.length)\">\n                            {{ getStatusText(index, currentScheduleData.times.length) }}\n                          </span>\n                        </div>\n                      </div>\n                    </div>\n                    <div v-else>\n                      <div \n                        v-for=\"(time, index) in currentScheduleData.times\" \n                        :key=\"index\" \n                        class=\"table-row\"\n                      >\n                        <div class=\"cell time-cell\">{{ time }}</div>\n                        <div class=\"cell status-cell\">\n                          <span class=\"status\" :class=\"getStatusClass(index, currentScheduleData.times.length)\">\n                            {{ getStatusText(index, currentScheduleData.times.length) }}\n                          </span>\n                        </div>\n                      </div>\n                    </div>\n                  </div>\n                </div>\n              </div>\n            </div>\n            <div v-else class=\"no-data\">\n              <p>该日期类型暂无班车信息</p>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: 'ShuttleBus',\n  data() {\n    return {\n      // 校区选择\n      fromCampus: 'balitai',\n      toCampus: 'jinnan',\n      \n      // 日期类型\n      scheduleType: 'weekday', // weekday, weekend, holiday\n      \n      // 当前日期\n      currentDate: new Date(),\n      \n      // 校车时刻表数据\n      busSchedules: {\n        balitaiToJinnan: {\n          weekday: {\n            express: {\n              route: '南开大学→天津图书馆→水上公园西路地铁站→天塔地铁站→南开大学新校西门→天南大新址公交站',\n              price: '5元',\n              times: ['7:50', '8:45', '9:00', '9:30', '10:00', '10:50', '11:30', '12:50', '13:10', '13:30', '13:50', '14:10', '14:50', '15:20', '15:50', '16:10', '16:50', '17:10', '17:30', '17:50', '18:20', '19:00', '19:20', '19:45', '20:00', '20:15', '20:30']\n            },\n            direct: {\n              route: '南开大学→一食堂（津南校区）→梨树园（津南校区）',\n              price: '5元',\n              times: ['6:30', '6:45', '7:00', '7:15', '7:30', '7:45', '8:00', '8:10', '8:20', '8:30', '10:30', '12:20', '12:30', '12:40', '14:30', '16:30', '18:30', '21:20']\n            }\n          },\n          weekend: {\n            express: {\n              route: '南开大学→天津图书馆→水上公园西路地铁站→天塔地铁站→南开大学新校西门→天南大新址公交',\n              price: '5元',\n              firstBus: '6:30',\n              lastBus: '21:15',\n              interval: '20-30分钟'\n            }\n          },\n          holiday: {\n            direct: {\n              route: '南开大学→一食堂（津南校区）→梨树园（津南校区）',\n              price: '5元',\n              times: ['6:00', '12:30', '18:30', '21:15']\n            }\n          }\n        },\n        jinnanToBalitai: {\n          weekday: {\n            express: {\n              route: '天南大新址公交站→南开大学新校西门→天塔地铁站→水上公园西路地铁站→天津图书馆→南开大学',\n              price: '5元',\n              times: ['6:30', '6:40', '6:50', '7:10', '7:40', '8:00', '8:30', '9:00', '9:40', '10:10', '10:30', '11:00', '11:20', '11:40', '12:10', '13:10', '14:00', '14:50', '15:10', '15:30', '15:50', '17:50', '18:30', '18:50', '19:10', '19:50', '20:10']\n            },\n            direct: {\n              route: '梨树园（津南校区）→一食堂（津南校区）→天塔地铁站→水上公园西路地铁站→天津图书馆→南开大学',\n              price: '5元',\n              times: ['6:35', '9:20', '12:20', '12:30', '12:40', '14:30', '16:00', '16:10', '16:25', '16:40', '17:00', '17:10', '17:20', '17:30', '18:10', '19:30', '20:35', '21:15']\n            }\n          },\n          weekend: {\n            express: {\n              route: '天南大新址公交站→南开大学新校西门→天塔地铁站→水上公园西路地铁站→天津图书馆→南开大学',\n              price: '5元',\n              firstBus: '6:30',\n              lastBus: '21:15',\n              interval: '20-30分钟'\n            }\n          },\n          holiday: {\n            direct: {\n              route: '梨树园（津南校区）→一食堂（津南校区）→天塔地铁站→水上公园西路地铁站→天津图书馆→南开大学',\n              price: '5元',\n              times: ['8:00', '12:30', '18:00', '21:15']\n            }\n          }\n        },\n        balitaiToTeda: {\n          weekday: {\n            route: '西门二道岗西区公寓→游泳馆东侧停车场→泰达学院钟楼',\n            price: '免费',\n            times: [\n              { departure: '6:25', location: '西门二道岗西区公寓前' },\n              { departure: '6:30', location: '游泳馆东侧停车场' },\n              { departure: '7:50', location: '西门二道岗西区公寓前' },\n              { departure: '8:00', location: '游泳馆东侧停车场' },\n              { departure: '10:00', location: '西门二道岗西区公寓前' },\n              { departure: '10:10', location: '游泳馆东侧停车场' },\n              { departure: '12:30', location: '西门二道岗西区公寓前' },\n              { departure: '12:40', location: '游泳馆东侧停车场' },\n              { departure: '17:20', location: '西门二道岗西区公寓前' },\n              { departure: '17:30', location: '游泳馆东侧停车场' }\n            ]\n          },\n          weekend: {\n            route: '西门二道岗西区公寓→游泳馆东侧停车场→泰达学院钟楼',\n            price: '免费',\n            times: [\n              { departure: '17:20', location: '西门二道岗西区公寓' },\n              { departure: '17:30', location: '游泳馆东侧停车场' }\n            ]\n          }\n        },\n        tedaToBalitai: {\n          weekday: {\n            route: '泰达学院钟楼→八里台校区',\n            price: '免费',\n            times: ['6:30']\n          },\n          weekend: {\n            route: '泰达学院钟楼→八里台校区',\n            price: '免费',\n            times: ['6:30']\n          }\n        },\n        jinnanToTeda: {\n          weekday: {\n            route: '津南校区梨树园→泰达学院钟楼',\n            price: '免费',\n            times: ['6:30', '8:30', '12:30', '18:00']\n          },\n          weekend: {\n            route: '津南校区梨树园→泰达学院钟楼',\n            price: '免费',\n            times: ['8:00', '11:00', '15:00', '18:00']\n          }\n        },\n        tedaToJinnan: {\n          weekday: {\n            route: '泰达学院钟楼→津南校区梨树园',\n            price: '免费',\n            times: ['6:30', '8:30', '12:30', '17:00']\n          },\n          weekend: {\n            route: '泰达学院钟楼→津南校区梨树园',\n            price: '免费',\n            times: ['8:00', '11:00', '15:00', '17:00']\n          }\n        }\n      },\n      \n      // 校区名称映射\n      campusNames: {\n        balitai: '八里台校区',\n        jinnan: '津南校区',\n        teda: '泰达校区'\n      }\n    }\n  },\n  computed: {\n    // 当前路线\n    currentRoute() {\n      return `${this.fromCampus}To${this.toCampus.charAt(0).toUpperCase() + this.toCampus.slice(1)}`;\n    },\n    \n    // 路线标题\n    routeTitle() {\n      return `${this.campusNames[this.fromCampus]} → ${this.campusNames[this.toCampus]}`;\n    },\n    \n    // 当前日期类型文本\n    scheduleTypeText() {\n      switch (this.scheduleType) {\n        case 'weekday':\n          return '工作日';\n        case 'weekend':\n          return '非工作日';\n        case 'holiday':\n          return '节假日';\n        default:\n          return '工作日';\n      }\n    },\n    \n    // 当前日期文本\n    currentDateText() {\n      const options = { year: 'numeric', month: 'long', day: 'numeric', weekday: 'long' };\n      return this.currentDate.toLocaleDateString('zh-CN', options);\n    },\n    \n    // 当前时刻表数据\n    currentScheduleData() {\n      const routeData = this.busSchedules[this.currentRoute];\n      if (!routeData) return null;\n      return routeData[this.scheduleType];\n    },\n    \n    // 是否需要显示上车地点列\n    needLocationColumn() {\n      if (!this.currentScheduleData || !this.currentScheduleData.times) return false;\n      return Array.isArray(this.currentScheduleData.times) && \n             this.currentScheduleData.times.length > 0 && \n             typeof this.currentScheduleData.times[0] === 'object' && \n             this.currentScheduleData.times[0].departure;\n    }\n  },\n  mounted() {\n    // 根据星期几设置默认的日期类型\n    const dayOfWeek = this.currentDate.getDay();\n    if (dayOfWeek === 0 || dayOfWeek === 6) {\n      this.scheduleType = 'weekend';\n    }\n  },\n  methods: {\n    // 交换校区\n    exchangeCampus() {\n      const temp = this.fromCampus;\n      this.fromCampus = this.toCampus;\n      this.toCampus = temp;\n    },\n    \n    // 查询班次\n    querySchedule() {\n      if (this.fromCampus === this.toCampus) {\n        this.$message({\n          message: '请选择不同的出发和目的校区',\n          type: 'error'\n        });\n        return;\n      }\n      this.$message({\n        message: '已更新路线时刻表',\n        type: 'success'\n      });\n    },\n    \n    // 改变日期类型\n    changeScheduleType(type) {\n      this.scheduleType = type;\n    },\n    \n    // 获取状态样式类\n    getStatusClass(index, total) {\n      if (index === total - 1) {\n        return 'status-last';\n      }\n      return 'status-available';\n    },\n    \n    // 获取状态文本\n    getStatusText(index, total) {\n      if (index === total - 1) {\n        return '末班车';\n      }\n      return '正常';\n    }\n  }\n}\n</script>\n\n<style scoped>\n/* 页面头部样式 */\n.page-header {\n    margin-bottom: 30px;\n    padding-bottom: 20px;\n    border-bottom: 1px solid #eee;\n}\n\n.breadcrumb {\n    font-size: 14px;\n    color: #666;\n    margin-bottom: 15px;\n}\n\n.breadcrumb a {\n    color: #666;\n    text-decoration: none;\n    transition: color 0.3s;\n}\n\n.breadcrumb a:hover {\n    color: var(--nk-blue);\n}\n\n.breadcrumb .separator {\n    margin: 0 8px;\n    color: #999;\n}\n\n.breadcrumb .current {\n    color: var(--nk-blue);\n    font-weight: 500;\n}\n\n.page-title {\n    font-size: 28px;\n    font-weight: 600;\n    color: #333;\n    margin-bottom: 10px;\n}\n\n.page-description {\n    font-size: 16px;\n    color: #666;\n    line-height: 1.5;\n}\n\n/* 校车查询容器 */\n.shuttle-bus-container {\n    display: flex;\n    flex-direction: column;\n    gap: 30px;\n}\n\n/* 路线查询部分 */\n.route-query-section {\n    display: grid;\n    grid-template-columns: 1fr 1fr;\n    gap: 30px;\n}\n\n.campus-selection {\n    background-color: #fff;\n    border-radius: var(--radius-lg);\n    box-shadow: var(--shadow-md);\n    padding: 25px;\n}\n\n.campus-selection h2 {\n    color: #333;\n    margin-bottom: 20px;\n    font-size: 20px;\n}\n\n.campus-selector {\n    display: flex;\n    align-items: flex-end;\n    gap: 15px;\n    margin-bottom: 25px;\n}\n\n.campus-from, .campus-to {\n    flex: 1;\n}\n\n.campus-from label, .campus-to label {\n    display: block;\n    margin-bottom: 8px;\n    font-weight: 500;\n    color: #333;\n}\n\n.campus-select {\n    width: 100%;\n    padding: 12px 15px;\n    border: 1px solid #ddd;\n    border-radius: var(--radius-sm);\n    font-size: 14px;\n    background-color: #fff;\n    cursor: pointer;\n    transition: border-color 0.3s;\n}\n\n.campus-select:focus {\n    border-color: var(--nk-blue);\n    outline: none;\n    box-shadow: 0 0 0 2px rgba(26, 83, 151, 0.2);\n}\n\n.exchange-icon {\n    display: flex;\n    align-items: center;\n    justify-content: center;\n    width: 40px;\n    height: 40px;\n    background-color: #f5f5f5;\n    border-radius: 50%;\n    color: var(--nk-blue);\n    cursor: pointer;\n    transition: var(--transition);\n    margin-bottom: 8px;\n}\n\n.exchange-icon:hover {\n    background-color: var(--nk-blue);\n    color: white;\n}\n\n.btn {\n    display: inline-flex;\n    align-items: center;\n    justify-content: center;\n    padding: 12px 20px;\n    border: none;\n    border-radius: var(--radius-sm);\n    font-size: 14px;\n    font-weight: 500;\n    cursor: pointer;\n    transition: var(--transition);\n    width: 100%;\n}\n\n.btn i {\n    margin-right: 8px;\n}\n\n.btn-primary {\n    background-color: var(--nk-blue);\n    color: white;\n}\n\n.btn-primary:hover {\n    background-color: var(--nk-blue-dark);\n}\n\n/* 校车信息提示 */\n.bus-info-tips {\n    background-color: #f8f9fa;\n    border-radius: var(--radius-lg);\n    padding: 20px;\n    border-left: 4px solid var(--nk-blue);\n}\n\n.tips-header {\n    display: flex;\n    align-items: center;\n    margin-bottom: 15px;\n    color: var(--nk-blue);\n}\n\n.tips-header i {\n    margin-right: 10px;\n    font-size: 18px;\n}\n\n.tips-header h3 {\n    margin: 0;\n    font-s