.timeline-content h3 {\n  color: var(--nk-blue-dark);\n  margin-bottom: 8px;\n  font-size: 16px;\n  font-weight: 700;\n}\n\n.timeline-content p {\n  color: var(--nk-gray-dark);\n  line-height: 1.6;\n  font-size: 14px;\n  margin: 0;\n}\n\n.culture-grid {\n  display: grid;\n  grid-template-columns: repeat(auto-fit, minmax(250px, 1fr));\n  gap: 32px;\n}\n\n.culture-item {\n  background-color: var(--nk-white);\n  border-radius: var(--radius-md);\n  padding: 36px;\n  text-align: center;\n  box-shadow: var(--shadow-sm);\n  transition: var(--transition);\n}\n\n.culture-item:hover {\n  transform: translateY(-5px);\n  box-shadow: var(--shadow-md);\n}\n\n.culture-icon {\n  width: 70px;\n  height: 70px;\n  background-color: rgba(26, 83, 151, 0.1);\n  border-radius: 50%;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  margin: 0 auto 24px;\n}\n\n.culture-icon i {\n  font-size: 1.8rem;\n  color: var(--nk-blue);\n}\n\n.culture-item h3 {\n  font-size: 22px;\n  margin-bottom: 16px;\n  color: var(--nk-blue-dark);\n}\n\n.culture-item p {\n  color: var(--nk-gray-dark);\n  line-height: 1.6;\n}\n\n.gallery {\n  display: grid;\n  grid-template-columns: repeat(auto-fit, minmax(300px, 1fr));\n  gap: 32px;\n}\n\n.gallery-item {\n  background-color: var(--nk-white);\n  border-radius: var(--radius-md);\n  overflow: hidden;\n  box-shadow: var(--shadow-sm);\n  transition: var(--transition);\n}\n\n.gallery-item:hover {\n  transform: translateY(-5px);\n  box-shadow: var(--shadow-md);\n}\n\n.gallery-image {\n  height: 200px;\n  overflow: hidden;\n}\n\n.gallery-image img {\n  width: 100%;\n  height: 100%;\n  object-fit: cover;\n  transition: transform 0.3s ease;\n}\n\n.gallery-item:hover .gallery-image img {\n  transform: scale(1.05);\n}\n\n.gallery-caption {\n  padding: 24px;\n  text-align: center;\n}\n\n.gallery-caption h3 {\n  font-size: 20px;\n  color: var(--nk-blue-dark);\n}\n\n@media (max-width: 992px) {\n  .timeline {\n    padding-left: 120px;\n  }\n  \n  .timeline::before {\n    left: 70px;\n  }\n  \n  .timeline-marker {\n    left: 61px;\n  }\n}\n\n@media (max-width: 768px) {\n  .page-title {\n    font-size: 36px;\n  }\n  \n  .page-subtitle {\n    font-size: 18px;\n  }\n  \n  .timeline {\n    padding-left: 100px;\n  }\n  \n  .timeline::before {\n    left: 60px;\n  }\n  \n  .timeline-marker {\n    left: 51px;\n  }\n  \n  .culture-grid,\n  .gallery {\n    grid-template-columns: 1fr;\n  }\n}\n\n@media (max-width: 576px) {\n  .content-card {\n    padding: 20px;\n  }\n}\n</style>"],"mappings":"AAoEA,eAAe;EACbA,IAAI,EAAE,WAAW;EACjBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,OAAO,EAAE,CACP;QACEC,IAAI,EAAE,OAAO;QACbC,KAAK,EAAE;MACT,CAAC,EACD;QACED,IAAI,EAAE,OAAO;QACbC,KAAK,EAAE;MACT,CAAC,EACD;QACED,IAAI,EAAE,OAAO;QACbC,KAAK,EAAE;MACT,CAAC,EACD;QACED,IAAI,EAAE,OAAO;QACbC,KAAK,EAAE;MACT,CAAC,EACD;QACED,IAAI,EAAE,OAAO;QACbC,KAAK,EAAE;MACT,CAAC,EACD;QACED,IAAI,EAAE,OAAO;QACbC,KAAK,EAAE;MACT,CAAC,EACD;QACED,IAAI,EAAE,OAAO;QACbC,KAAK,EAAE;MACT,CAAC,EACD;QACED,IAAI,EAAE,OAAO;QACbC,KAAK,EAAE;MACT,EACD;MACDC,OAAO,EAAE,CACP;QACEC,IAAI,EAAE,uBAAuB;QAC7BC,KAAK,EAAE,IAAI;QACXC,WAAW,EAAE;MACf,CAAC,EACD;QACEF,IAAI,EAAE,aAAa;QACnBC,KAAK,EAAE,IAAI;QACXC,WAAW,EAAE;MACf,CAAC,EACD;QACEF,IAAI,EAAE,cAAc;QACpBC,KAAK,EAAE,IAAI;QACXC,WAAW,EAAE;MACf,CAAC,EACD;QACEF,IAAI,EAAE,cAAc;QACpBC,KAAK,EAAE,MAAM;QACbC,WAAW,EAAE;MACf,EACD;MACDC,OAAO,EAAE,CACP;QACEF,KAAK,EAAE,IAAI;QACXG,KAAK,EAAEC,OAAO,CAAC,yBAAyB;MAC1C,CAAC,EACD;QACEJ,KAAK,EAAE,KAAK;QACZG,KAAK,EAAEC,OAAO,CAAC,4BAA4B;MAC7C,CAAC,EACD;QACEJ,KAAK,EAAE,KAAK;QACZG,KAAK,EAAEC,OAAO,CAAC,yBAAyB;MAC1C,CAAC,EACD;QACEJ,KAAK,EAAE,MAAM;QACbG,KAAK,EAAEC,OAAO,CAAC,yBAAyB;MAC1C;IAEJ;EACF;AACF","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}